{
	"name": "qml-format",
	"displayName": "qml-format",
	"description": "An extension to beautify Qt QML files",
	"publisher": "Delgan",
	"repository": {
		"url": "https://github.com/Delgan/qml-format",
		"type": "git"
	},
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:qml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "qml",
				"aliases": [
					"QML"
				],
				"extensions": [
					"qml"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.69.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4"
	}
}
